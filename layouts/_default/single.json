{{ define "response" }}
		{
		    "datetime": {{ .Date.Unix }},
		    "versionCode" : {{ .Title }},
		    "versionName" : "{{ .Params.VersionName }}",
		    "filename": "{{ .Params.FileName }}",
		    {{ with .Params.SecurityPatchLevel }}"spl": "{{ dateFormat "2006-01-02" . }}",{{ end }}
		    "id": "{{ .Params.MD5 }}",
		    "romtype" : "{{ .Params.ReleaseType }}",
		    "size" : {{ .Params.Size }},
		    "url" : "{{ .Params.FileURL }}",
		    "changelog" : {{ .RawContent | jsonify }}
		}
{{ end }}